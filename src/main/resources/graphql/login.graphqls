type Query {
  saludo: String
}

type Mutation {
  login(input: LoginInput!): String
  logout: String 
}

input LoginInput {
  documento: String!
  password: String!
  tipoDocumento: TipoDocumento!
}

enum TipoDocumento {
  CEDULA_CIUDADANIA
  TARJETA_IDENTIDAD
  CEDULA_EXTRANJERIA
  PASAPORTE
  PERMISO_ESPECIAL_PERMANENCIA
}
